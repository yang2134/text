.shadow-blur {
  position: relative;
}
.shadow-blur::before {
  content: "";
  display: block;
  background: inherit;
  filter: blur(3px);
  position: absolute;
  width: 100%;
  height: 100%;
  top: 3px;
  left: 3px;
  z-index: -1;
  opacity: 0.4;
  transform-origin: 0 0;
  border-radius: inherit;
  transform: scale(1, 1);
}

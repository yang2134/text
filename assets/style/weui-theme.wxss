.weui-tab .weui-navbar[class*="bg-"] {
  border-bottom: 0;
}
.weui-tab .weui-navbar[class*="bg-"] .weui-navbar__item {
  color: #fff;
}
.weui-tab .weui-navbar[class*="bg-"] .weui-navbar__item:active {
  background-color: transparent;
}
.weui-tab .weui-navbar[class*="bg-"] .on:after {
  width: 100%;
  border-bottom: 1px solid var(--white);
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 1px;
}
.weui-tab .weui-navbar[class*="bg-gray"] .weui-navbar__item {
  color: var(--black) !important;
}
.weui-tab .weui-navbar[class*="bg-gray"] .on:after {
  border-bottom: 1px solid var(--black);
}
.weui-tab .weui-navbar[class*="bg-white"] .weui-navbar__item {
  color: var(--black) !important;
}
.weui-tab .weui-navbar[class*="bg-white"] .on:after {
  border-bottom: 1px solid var(--black);
}
.weui-tab .weui-navbar.flex-center {
  justify-content: center;
}
.weui-tab .weui-navbar.flex-center .weui-navbar__item {
  padding: 5px 10px;
  flex: 0 0 auto;
}
.weui-vertical-tab .weui-vertical-navbar[class*="bg-"] .weui-vertical-navbar__item {
  border-bottom: none;
  color: #fff;
}
.weui-vertical-tab .weui-vertical-navbar[class*="bg-"] .weui-vertical-navbar__item:active {
  background-color: transparent;
}

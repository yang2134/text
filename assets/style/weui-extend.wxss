  justify-content: space-between;
  align-items: center;
}
.quantity-decrease,
.quantity-increase {
  cursor: pointer;
}
.quantity-decrease,
.quantity-increase {
  display: inline-block;
  position: relative;
  width: 25px;
  height: 100%;
  border: 1px solid transparent;
  border-radius: 4px;
  border-color: #ccc;
  box-sizing: border-box;
}
.quantity {
  display: inline-block;
  position: relative;
  width: 25px;
  height: 100%;
  border: 1px solid transparent;
  border-radius: 4px;
  border-color: #ccc;
  box-sizing: border-box;
  -webkit-appearance: none;
  text-align: center;
  width: 34px;
  vertical-align: top;
  font-size: 14px;
  line-height: 24px;
  color: #232326;
  min-height: 0;
}
.quantity-decrease {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  margin-left: 0;
}
.quantity-decrease:before {
  content: "";
  position: absolute;
  top: 11px;
  left: 6px;
  width: 12px;
  height: 1px;
  background-color: #ccc;
}
.quantity-increase {
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
  margin-left: -1px;
}
.quantity-increase:before {
  content: "";
  position: absolute;
  top: 11px;
  left: 6px;
  width: 12px;
  height: 1px;
  background-color: #ccc;
}
.quantity-increase:after {
  content: "";
  position: absolute;
  top: 6px;
  left: 11px;
  width: 1px;
  height: 12px;
  background-color: #ccc;
}
.quantity {
  border-radius: 0;
  margin-left: -1px;
}
